{
  "name": "nba-analyst",
  "version": "0.1.0",
  "description": "NBA Analyst - AI-powered sports betting decision platform",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src vitest.config.mts",
    "lint:next": "next lint",
    "typecheck": "tsc --noEmit",
    "check:ml-governance": "node scripts/check-ml-governance.js",
    "verify:hardening": "npm run lint && npm run typecheck && npm run typecheck:security && npm run test:security && npm run test:b2b-docs && npm run test:ml-core && npm run check:ml-governance",
    "verify:hardening:ci": "npm run test:security && npm run test:b2b-docs && npm run test:ml-core && npm run check:ml-governance",
    "test": "vitest run",
    "test:contracts": "vitest run src/server/docs/openapi-contract.test.ts src/server/cache/rate-limiter.test.ts",
    "test:security": "vitest run src/server/cache/rate-limiter.test.ts src/server/docs/openapi-contract.test.ts src/app/api/auth/login/route.test.ts src/app/api/auth/me/route.test.ts src/app/api/v1/decisions/route.test.ts src/app/api/v1/b2b/_base.test.ts src/app/api/v1/b2b/schemas.test.ts src/app/api/v1/b2b/decisions/route.test.ts src/app/api/v1/b2b/decisions/[id]/route.test.ts src/app/api/v1/b2b/decisions/[id]/explain/route.test.ts src/app/api/v1/b2b/profiles/route.test.ts src/app/api/v1/b2b/profiles/[id]/route.test.ts src/app/api/v1/b2b/profiles/[id]/history/route.test.ts src/app/api/v1/b2b/runs/route.test.ts",
    "test:b2b-docs": "vitest run src/app/api/v1/b2b/openapi/route.test.ts src/app/api/v1/b2b/docs/route.test.ts",
    "test:ml-core": "vitest run src/server/ml/prediction/prediction-service.test.ts",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ci": "playwright test --ci",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "test:install-deps": "playwright install-deps",
    "test:p0": "playwright test --grep @p0",
    "test:p1": "playwright test --grep @p1",
    "test:p0-p1": "playwright test --grep \"@p0|@p1\"",
    "test:smoke": "playwright test --grep @smoke",
    "test:mobile": "playwright test --grep @mobile",
    "test:a11y": "playwright test --grep @a11y",
    "test:regression": "playwright test --grep @regression",
    "test:nightly": "playwright test --grep @nightly",
    "lighthouse": "lhci autorun",
    "lighthouse:ci": "lhci autorun --config=lighthouserc.js",
    "ml": "node scripts/ml-cli.js",
    "ml:install": "npm run ml install",
    "ml:train": "npm run ml train -- --activate",
    "ml:fetch-data": "npm run ml fetch-data",
    "ml:activate": "npm run ml activate",
    "ml:status": "npm run ml status",
    "ml:dashboard": "npm run ml dashboard",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "npx tsx prisma/seed.ts",
    "data:fetch": "npx tsx scripts/fetch-historical-data-full.ts",
    "data:fetch-test": "npm run data:fetch -- --start-date=2024-10-22 --end-date=2024-10-31 --season=2024 --include-players --update-rosters",
    "data:fetch-2024": "npm run data:fetch -- --start-date=2024-10-22 --end-date=2025-06-17 --season=2024 --include-players --include-injuries --update-rosters",
    "data:fetch-deep": "npx tsx scripts/fetch-deep-data.ts",
    "data:fetch-resume": "npx tsx scripts/fetch-deep-data.ts resume",
    "data:fetch-status": "npx tsx scripts/fetch-deep-data.ts status",
    "data:features": "npx tsx scripts/fetch-deep-data.ts features",
    "ml:train-full": "npx tsx scripts/train-ml-model-full.ts",
    "ml:train-basic": "npm run ml:train-full -- --start-date=2015-10-27 --end-date=2026-02-16 --activate",
    "ml:train-advanced": "npm run ml:train-full -- --start-date=2015-10-27 --end-date=2026-02-16 --include-player-features --feature-set=full --activate",
    "ml:legacy:train-py": "node scripts/ml-legacy-wrapper.js train-ml-model.py",
    "ml:legacy:train-v2": "node scripts/ml-legacy-wrapper.js train-ml-model-v2.py",
    "ml:legacy:train-v3": "node scripts/ml-legacy-wrapper.js train-ml-model-v3.py",
    "stats:ml": "npx tsx scripts/show-ml-stats.ts",
    "predict:end-of-season": "npx tsx scripts/predict-season-end-espn.ts",
    "import:github": "npx tsx scripts/import-github-data.ts",
    "import:kaggle": "npx tsx scripts/import-kaggle-odds.ts",
    "import:injuries": "npx tsx scripts/import-historical-injuries.ts",
    "pipeline:evening": "npx tsx scripts/evening-pipeline.ts",
    "pipeline:pre-game": "npx tsx scripts/evening-pipeline.ts pre-game",
    "pipeline:post-game": "npx tsx scripts/post-game-pipeline.ts",
    "cache:clear": "npx tsx scripts/clear-cache.ts",
    "cache:stats": "npx tsx scripts/cache-stats.ts",
    "typecheck:security": "tsc -p tsconfig.security.json --noEmit"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.4.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-virtual": "^3.13.18",
    "axios": "^1.13.5",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.3.1",
    "duckdb": "^1.4.4",
    "lru-cache": "^11.2.6",
    "lucide-react": "^0.564.0",
    "next": "^15.1.0",
    "next-auth": "^4.24.13",
    "openapi-types": "^12.1.3",
    "pino": "^9.6.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "redis": "^4.7.0",
    "tailwind-merge": "^3.4.1",
    "totp-generator": "^2.0.1",
    "web-vitals": "^3.5.2",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.1",
    "@faker-js/faker": "^9.5.0",
    "@lhci/cli": "^0.14.0",
    "@playwright/test": "^1.50.0",
    "@seontechnologies/playwright-utils": "^4.0.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20.17.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/ui": "^4.0.18",
    "autoprefixer": "^10.4.20",
    "csv-parse": "^6.1.0",
    "eslint": "^9.20.0",
    "eslint-config-next": "^15.1.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.5.0",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
